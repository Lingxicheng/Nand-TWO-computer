/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not (in=a, out=Nota);
	Not (in=b, out=Notb);
	And (a=a, b=Notb, out=Notbanda);
	And (a=Nota, b=b, out=Notaandb);
	Or (a=Notbanda, b=Notaandb, out=out);

}